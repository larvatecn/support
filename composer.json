    "ext-curl": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*",
    "ext-intl": "*",
    "ext-fileinfo": "*",
    "nesbot/carbon": "^2.31",
    "guzzlehttp/guzzle": "^6.5 || ^7.2",
